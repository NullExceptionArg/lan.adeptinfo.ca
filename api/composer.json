{
  "name": "adept-informatique/lan",
  "description": "API du site web du LAN de l'ADEPT",
  "keywords": [
    "lan-party",
    "Web-API",
    "lumen"
  ],
  "license": "GPL-3.0",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "laravel/lumen-framework": "5.8.*",
    "vlucas/phpdotenv": "^3.3",
    "flipbox/lumen-generator": "^5.6",
    "laravel/passport": "7.0.3",
    "dusterio/lumen-passport": "^0.2.9",
    "barryvdh/laravel-cors": "^0.11.2",
    "dingo/api": "2.2.3",
    "guzzlehttp/guzzle": "6.3.3",
    "seatsio/seatsio-php": "^39.0",
    "predis/predis": "^1.1",
    "league/flysystem": " ~1.0",
    "caouecs/laravel-lang": "~3.0",
    "illuminate/mail": "^5.7",
    "google/apiclient": "^2.2",
    "ext-json": "*",
    "facebook/graph-sdk": "^5.7"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.8",
    "phpunit/phpunit": "~7.5",
    "mockery/mockery": "~1.2",
    "laravel/homestead": "^7.20"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
